RightCard = ({ info, handleClick }) => {\n    return (\n      <div className=\"quiz-card\" onClick={handleClick}>\n        <div class=\"center\">\n            { info.side_b }\n        </div>\n      </div>\n    )\n}\n\nconst mapStateToProps = state => {\n    return {\n      apiInfo: state.apiInfo,\n      session: state.session,\n      game: state.game\n    }\n}\n  \nexport default connect(mapStateToProps, { fetchCards })(WholeGame)\n// export default WholeGame\n","/Users/elicusic/Desktop/react_project/connect-four/src/components/MuppetLi.js",[],"/Users/elicusic/Desktop/react_project/connect-four/src/reducers/sessionReducer.js",[],"/Users/elicusic/Desktop/react_project/connect-four/src/containers/SidePanel.js",[],"/Users/elicusic/Desktop/react_project/connect-four/src/containers/MainContainer.js",[],"/Users/elicusic/Desktop/react_project/connect-four/src/components/ProfilePic.js",["171"],"import React from 'react'\n\nconst ProfilePic = ({ image }) => {\n    return (\n      <div id=\"profile-pic\" class=\"center three-percent\">\n        <img class=\"circle responsive-img\" src={image}></img>\n      </div>\n    )\n}\n\nexport default ProfilePic","/Users/elicusic/Desktop/react_project/connect-four/src/components/DeckSelector.js",[],"/Users/elicusic/Desktop/react_project/connect-four/src/components/GoToGameButton.js",["172"],"import React from 'react'\nimport { connect } from 'react-redux'\n\nconst GoToGameButton = ({ cards, initializeGame }) => {\n    return (\n      <div class=\"three-percent center\" id=\"button-div\">\n        <button class=\"btn white-smoke waves-effect\" onClick={() => initializeGame(cards) }>Go To Game</button>\n      </div>\n    )\n}\n\nexport default GoToGameButton","/Users/elicusic/Desktop/react_project/connect-four/src/components/GameSelector.js",[],"/Users/elicusic/Desktop/react_project/connect-four/src/components/HomeCard.js",["173"],"import React from 'react'\n\nconst HomeCard = () => {\n    return (\n      <div id=\"home-card\">\n        <